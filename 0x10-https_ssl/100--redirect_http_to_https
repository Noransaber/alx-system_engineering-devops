global
    log 127.0.0.1 local0 notice

defaults
    mode http
    log global
    option httplog
    option dontlognull
    timeout connect 10s
    timeout client 1m
    timeout server 1m

frontend https
    bind *:443
    ssl crt /etc/ssl/certs/www.example.com.pem
    ssl key /etc/ssl/private/www.example.com.key
    default_backend webservers

backend webservers
    balance roundrobin
    server web01 192.168.1.101:80
    server web02 192.168.1.102:80
